<a href="/">Home</a> <a href="/promoters_signup">Signup!</a>
<h1>Promoter Login</h1>
<%= form_tag '/promoters_login', method: :post do %>
  <div class="form-group">
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>
  <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <%= submit_tag "Submit", class:"btn btn-primary" %>
<% end %>